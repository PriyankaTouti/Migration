SELECT * INTO IDLIST_BKP20062025 FROM IDLIST
SELECT * INTO CIS_SyncIDList_BKP20062025 FROM CIS_SyncIDList
SELECT * INTO EE_IDList_BKP20062025 FROM EE_IDList
SELECT * INTO MashupIdList_BKP20062025 FROM MashupIdList
 
UPDATE IDLIST SET LastID=ISNULL(LastID,0)+10000;
UPDATE CIS_SyncIDList SET LastID=ISNULL(LastID,0)+10000;
UPDATE EE_IDList SET LastID=ISNULL(LastID,0)+10000;
UPDATE MashupIdList SET LastID=ISNULL(LastID,0)+10000;
