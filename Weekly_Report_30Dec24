/*Total Application*/
select count(1) from FinancialApplication where ownerid = 985  --42842

/*Weekly count for application*/
select count(1) from FinancialApplication where ownerid = 985 and convert(date,createdon)>='2024-12-23' and convert(date,createdon)<='2024-12-29'  --667

/*Statuscode wise application count*/
select s.StatusCode,count(*) from FinancialApplication f
left outer join statuscodemaster s on f.OwnerId = s.OwnerID and f.StatusCodeId = s.StatusCodeID 
where f.ownerid = 985 group by s.StatusCodeID,s.StatusCode  having COUNT(s.StatusCode)>1 order by s.StatusCodeID

/*Sent for Post Approval Finalization*/
select count(1) from FinancialApplication where ownerid = 985 and statuscodeid=100024    --3330

/*Sent for Pre-Booking*/
select count(1) from FinancialApplication where ownerid = 985 and statuscodeid=100026   --86

/*Salesman Count*/
select count(1) from accounts where ownerid = 985 and layoutid = 10204035     --1031

/*Sub-Salesman   Count*/
select count(1) from Relationship where ownerid = 985 and layoutid = 10204032     --0

/*Dealer  Count*/
select count(1) from accounts where ownerid = 985 and layoutid = 10204025 	  --276

/*Sun-Dealer Count*/
select count(1) from Relationship where ownerid = 985 and layoutid = 10204050     --118

/*Total MC Printing*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'MC'  group by  itemid having ITEMID >1  --1852

/*Weekly MC Printing*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'MC' and convert(date,insertedon)>='2024-12-23' and convert(date,insertedon)<='2024-12-29' group by  itemid having ITEMID >1  --153

/*Monthly MC Printing*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'MC' and convert(date,insertedon)>='2024-12-01' and convert(date,insertedon)<='2024-12-31' group by  itemid having ITEMID >1  --394

/*TOTAL CIF Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'CIF'  group by  itemid having ITEMID >1  --6336

/*Weekly CIF Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'CIF' and convert(date,insertedon)>='2024-12-23' and convert(date,insertedon)<='2024-12-29' group by  itemid having ITEMID >1  --541

/*Monthly CIF Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'CIF' and convert(date,insertedon)>='2024-12-01' and convert(date,insertedon)<='2024-12-31' group by  itemid having ITEMID >1  --1340

/*Total LAN Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'LAN'   group by  itemid having ITEMID >1  --3351

/*Weekly LAN Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'LAN'  and convert(date,insertedon)>='2024-12-23' and convert(date,insertedon)<='2024-12-29' group by  itemid having ITEMID >1  --274

/*Monthly LAN Created*/
select count(1)  from MPI_MQ_REQUEST_LOG where REQUESTTYPE = 'LAN'  and convert(date,insertedon)>='2024-12-01' and convert(date,insertedon)<='2024-12-31' group by  itemid having ITEMID >1  --678

/*Total Amount Disbursed*/
select sum(fia_ex1_21) as Total_amt_Disbursed from FinancialApplication f
inner join fia_Ex1 f1 on f.ownerid = f1.ownerid and f.CustomObjectId =f1.fia_ex1_id
where f.ownerid =985 and f.StatusCodeId = 100036  --660769779.62

/*User Login*/
select format(dateadd(hour,08,LoginDate),'yyyy-MM-dd'),count(distinct userid)   from LoginHistory  where OwnerID = 985  and convert(date,LoginDate)>='2024-04-18' and convert(date,dateadd(hour,08,LoginDate))<='2024-12-29' 
group by format(dateadd(hour,08,LoginDate),'yyyy-MM-dd')   order by format(dateadd(hour,08,LoginDate),'yyyy-MM-dd')
